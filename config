general {
	colors = true
	interval = 5
	}

order += "wireless _first_"
order += "ethernet _first_"
order += "tztime local"

wireless _first_ {
	  format_up = "W: %essid %ip"
	  format_down = "W: down"
}

ethernet _first_ {
	 format_up = "E: %ip (%speed)"
	 format_down = "E: down"
}

tztime local {
       format = "%Y-%m-%d %H:%M:%S"
}
